<!DOCTYPE html>
<html lang="en">
<% include header %>
<body class="hold-transition sidebar-mini">
<div class="wrapper">

  <!-- Navbar -->
  <% include navbar %>
  <!-- /.navbar -->

  <!-- Main Sidebar Container -->
  <% include aside %>

  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <div class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-sm-6">
            <h1 class="m-0"><%= heading %></h1>
          </div><!-- /.col -->
          
        </div><!-- /.row -->
      </div><!-- /.container-fluid -->
    </div>
    <!-- /.content-header -->

    <!-- Main content -->
    <div class="content">
      <div class="container-fluid">
        <%- include(content) %>
      </div><!-- /.container-fluid -->
    </div>
    <!-- /.content -->
  </div>
  <!-- /.content-wrapper -->

  <!-- Control Sidebar -->
  <% include control-sidebar %>
  <!-- /.control-sidebar -->

  <!-- Main Footer -->
  <% include footer %>
</div>
<!-- ./wrapper -->

<!-- REQUIRED SCRIPTS -->

<!-- jQuery -->
<script src="<%= process.env.BASE_URL %>plugins/jquery/jquery.min.js"></script>
<!-- Bootstrap 4 -->
<script src="<%= process.env.BASE_URL %>plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
<!-- AdminLTE App -->
<script src="<%= process.env.BASE_URL %>dist/js/adminlte.min.js"></script>
<!-- Datatable plugin -->
<script src="<%= process.env.BASE_URL %>plugins/datatables/jquery.dataTables.min.js"></script>
<script src="<%= process.env.BASE_URL %>plugins/datatables-bs4/js/dataTables.bootstrap4.min.js"></script>
<script src="<%= process.env.BASE_URL %>plugins/datatables-responsive/js/dataTables.responsive.min.js"></script>
<script src="<%= process.env.BASE_URL %>plugins/datatables-responsive/js/responsive.bootstrap4.min.js"></script>
<script>
  $(document).ready( function () {
    $('#dataTable1').DataTable();
    $('#dataTable2').DataTable();
    $('#dataTable3').DataTable();
    $('#dataTable4').DataTable();
    $('#dataTable5').DataTable();
  });
</script>
</body>
</html>
